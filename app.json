{
  "expo": {
    "name": "Poynt",
    "slug": "Poynt",
    "version": "1.0.0",
    "scheme": "Poynt",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logs.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiYWxwaGFyIiwiYSI6ImNtMHZncmNpdjByMW8ya3FzcnJuazNiNmwifQ.2gHQ9mH6wKjRdGJeotJ14Q",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logs.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/trans.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF6E6"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ts2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "package": "com.alphar33.Poynt"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6e5b05bf-58b6-467c-afa3-4f126091ec35"
      }
    },
    "assets": ["./assets/trans.png"]
  }
}
