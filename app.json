{
  "expo": {
    "name": "poynt",
    "slug": "poynt",
    "version": "1.0.0",
    "scheme": "poynt",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logs.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiYWxwaGFyIiwiYSI6ImNtMHZncmNpdjByMW8ya3FzcnJuazNiNmwifQ.2gHQ9mH6wKjRdGJeotJ14Q",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "react-native-video"
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logs.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/trans.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF6E6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paultech.poynt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ts2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "package": "com.paultech.poynt"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5689d13b-d741-4d3b-92d9-872f12cd795c"
      }
    },
    "assets": [
      "./assets/trans.png"
    ]
  }
}
